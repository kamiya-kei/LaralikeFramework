{
    "name": "laralike/laralike-framework",
    "description": "project like laravel",
    "type": "project",
    "require": {
        "php": "^7.1",
        "laralike/laralike-router": "^1.0",
        "twig/twig": "^2.12",
        "illuminate/database": "^5.5",
        "illuminate/events": "^5.2",
        "vlucas/phpdotenv": "^4.1",
        "monolog/monolog": "^1.25"
    },
    "require-dev": {
        "digitalnature/php-ref": "^1.3",
        "filp/whoops": "^2.7",
        "phpunit/phpunit": "^7.5",
        "guzzlehttp/guzzle": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": [
            "php ./vendor/phpunit/phpunit/phpunit --colors"
        ],
        "updtautoload": [
            "composer dump-autoload"
        ],
        "serve": [
            "php -S localhost:8008 -t ./"
        ]
    }
}
